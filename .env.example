# Gemini API Key
# Get your key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key

# Supabase (client + service role)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Cloud object storage for uploads/manifests
CLOUD_STORAGE_BUCKET=your_bucket_name
CLOUD_STORAGE_REGION=your_bucket_region
CLOUD_STORAGE_ACCESS_KEY=your_access_key
CLOUD_STORAGE_SECRET_KEY=your_secret_key

# Optional: S3-compatible endpoint (e.g., Google Cloud Storage or R2)
CLOUD_STORAGE_ENDPOINT=https://storage.googleapis.com
# Optional: Force path-style URLs for providers that need it
CLOUD_STORAGE_FORCE_PATH_STYLE=true
