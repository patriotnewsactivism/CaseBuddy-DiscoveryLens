# Gemini API Key
# Get your key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key

# Optional: Override default Gemini models
# GEMINI_ANALYSIS_MODEL=gemini-2.0-pro-exp-02-05
# GEMINI_CHAT_MODEL=gemini-2.0-pro-exp-02-05
# GEMINI_TRANSCRIBE_MODEL=gemini-2.0-flash-001

# Optional: Rate limiting and caching configuration
# GEMINI_ENABLE_CACHE=true
# GEMINI_ENABLE_RATE_LIMIT=true
# GEMINI_CACHE_TTL_MS=3600000

# Supabase (client + service role)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Cloud object storage for uploads/manifests (Google Cloud Storage)
CLOUD_STORAGE_BUCKET=your_bucket_name
CLOUD_STORAGE_REGION=us-east1
CLOUD_STORAGE_ACCESS_KEY=your_access_key
CLOUD_STORAGE_SECRET_KEY=your_secret_key
CLOUD_STORAGE_ENDPOINT=https://storage.googleapis.com
CLOUD_STORAGE_FORCE_PATH_STYLE=true

# AssemblyAI (optional - for audio/video transcription)
# ASSEMBLYAI_API_KEY=your_assemblyai_key
# ASSEMBLYAI_SPEECH_MODEL=universal
