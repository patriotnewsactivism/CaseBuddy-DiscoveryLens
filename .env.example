# OpenAI API Key
# Get your key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key

# Optional: Override default OpenAI models
# OPENAI_ANALYSIS_MODEL=gpt-4o
# OPENAI_CHAT_MODEL=gpt-4o

# Optional: Caching configuration
# AI_ENABLE_CACHE=true
# AI_CACHE_TTL_MS=3600000

# Supabase (client + service role)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Cloud object storage for uploads/manifests (Google Cloud Storage)
CLOUD_STORAGE_BUCKET=your_bucket_name
CLOUD_STORAGE_REGION=us-east1
CLOUD_STORAGE_ACCESS_KEY=your_access_key
CLOUD_STORAGE_SECRET_KEY=your_secret_key
CLOUD_STORAGE_ENDPOINT=https://storage.googleapis.com
CLOUD_STORAGE_FORCE_PATH_STYLE=true

# Azure Document Intelligence (OCR for scanned PDFs)
# Get your endpoint and key from: https://portal.azure.com
AZURE_DOC_INTELLIGENCE_ENDPOINT=https://your-resource.cognitiveservices.azure.com/
AZURE_DOC_INTELLIGENCE_KEY=your_azure_doc_intelligence_key

# AssemblyAI (optional - for audio/video transcription)
# ASSEMBLYAI_API_KEY=your_assemblyai_key
# ASSEMBLYAI_SPEECH_MODEL=universal
